
@{
    Layout = null;
}
@using EF.Application.Model;
@model EF.Application.Model.t_function

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link href="~/Content/common.css" rel="stylesheet" />
    <link href="~/Scripts/layui/css/layui.css" rel="stylesheet" />
    <link href="~/Content/style.css" rel="stylesheet" />
    <script src="~/Scripts/echarts/echarts.js"></script>
    <script src="~/Scripts/jquery-1.8.0.min.js"></script>
</head>
<body>
    <blockquote class="layui-elem-quote layui-text">
        菜单详情
    </blockquote>
    <div class="layui-form-item">
        <div class="layui-inline">
            <label class="layui-form-label">功能名称</label>
            <div class="layui-input-block">
                @Html.EditorFor(model => model.functionName, new { htmlAttributes = new { @class = "layui-input", @readonly = "readonly" } })
            </div>
        </div>
    </div>
    <div class="layui-form-item">
        <div class="layui-inline">
            <label class="layui-form-label">功能地址</label>
            <div class="layui-input-block">
                @Html.EditorFor(model => model.actionAddress, new { htmlAttributes = new { @class = "layui-input", @readonly = "readonly" } })
            </div>
        </div>
    </div>
    <div class="layui-form-item">
        <div class="layui-inline">
            <label class="layui-form-label">Icon图标</label>
            <div class="layui-input-block">
                @Html.EditorFor(model => model.iconFont, new { htmlAttributes = new { @class = "layui-input", @readonly = "readonly" } })
            </div>
        </div>
    </div>
    <div class="layui-form-item">
        <div class="layui-inline">
            <label class="layui-form-label">排序号</label>
            <div class="layui-input-block">
                @Html.EditorFor(model => model.orderNo, new { htmlAttributes = new { @class = "layui-input", @readonly = "readonly" } })
            </div>
        </div>
    </div>
    <div class="layui-form-item layui-form-text">
        <label class="layui-form-label">功能说明</label>
        <div class="layui-input-block">
            @Html.TextAreaFor(model => model.functionDescription, new { @class = "layui-textarea", @readonly = "readonly" })
        </div>
    </div>
    <div class="layui-input-block">
        <button class="layui-btn layui-btn-primary" onclick="window.history.back()">返回</button>
    </div>
    <script src="~/Scripts/layui/layui.js"></script>
</body>
</html>
